{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quote-props": ["error", "as-needed"],
        "prefer-template": "warn",
        "object-curly-spacing": ["warn", "always"],
        "require-jsdoc": "off",
        "new-cap": "off",
        "max-len": "off",
        "no-invalid-this": "off"
    }
}
